stock IsAnyoneInVehicle(vehicleid)
{
    foreach(Player, i)
    {
        if(IsPlayerInAnyVehicle(i) && GetPlayerVehicleID(i) == vehicleid)
        {
            return 1;
        }
    }
    return 0;
}
